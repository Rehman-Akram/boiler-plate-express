{
  "name": "boiler-plate-express",
  "version": "1.0.0",
  "description": "This is express boiler plate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "start-dev": "nodemon node app.js",
    "run-migrations": "npx sequelize-cli db:migrate",
    "revert-migration": "npx sequelize-cli db:migrate:undo",
    "create-migration": "npx sequelize-cli migration:generate --name",
    "create-seed": "npx sequelize-cli seed:generate --name",
    "run-seed": "npx sequelize-cli db:seed:all",
    "seed-single": "npx sequelize-cli db:seed --seed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rehman-Akram/boiler-plate-express"
  },
  "keywords": [
    "express",
    "boiler",
    "plate"
  ],
  "author": "Rehman Akram",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "i": "^0.3.7",
    "nodemon": "^3.1.0",
    "npm": "^10.5.0",
    "sequelize-cli": "^6.6.2"
  }
}
